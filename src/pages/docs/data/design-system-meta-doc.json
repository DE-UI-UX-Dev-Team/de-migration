                {
                    "tag": "title",
                    "value": "Flex Grid - Row"
                },
                {
                    "tag": "description",
                    "value": "Nested within a `.container` or a `.col` (column), rows are encapsulates columns for the purpose of negating the far left and far right padding or gutter of the first and last columns"
                },
                {
                    "tag": "filter-selector",
                    "value": ".row"
                },
                {
                    "tag": "characteristic",
                    "value": "[display] Rows by default use `display:flex;`"
                },
                {
                    "tag": "characteristic",
                    "value": "[horizontal-spacing] Rows have negative margins on the left and right"
                },
                {
                    "tag": "characteristic",
                    "value": "[vertical-spacing] By design rows do not have margins on the top or bottom. This is enforced by !important."
                },
                {
                    "tag": "characteristic",
                    "value": "[background-color] When background colors are applied to a row margin left and right are set to 0"
                },
                {
                    "tag": "tags",
                    "value": "grid, row, column, flexbox"
                },
                {
                    "tag": "uri-demo-page",
                    "value": "/demos/flex-grid/"
                },
                {
                    "tag": "uri-related-links",
                    "value": "[Container] #content-container, [Columns] #flex-grid---columns"
                }
            ],
            "styles": {
                ".row": {
                    "margin-left": "calc(var(--gutter-xl) * -1)",
                    "margin-right": "calc(var(--gutter-xl) * -1)",
                    "margin-bottom": "0 !important",
                    "margin-top": "0 !important",
                    "display": "flex",
                    "flex-direction": "row",
                    "flex-wrap": "wrap"
                },
                ".row[class*=bg]": {
                    "margin-left": "0",
                    "margin-right": "0"
                },
                ".row[class*=bg] .col": {
                    "padding-left": "calc(var(--gutter-xl) * 2)",
                    "padding-right": "calc(var(--gutter-xl) * 2)"
                },
                ".row.even-3--sm > [class*=col]": {
                    "flex": "0 1 33.3333333333%"
                },
                ".row.even-4--sm > [class*=col]": {
                    "flex": "0 1 25%"
                },
                ".row.even-5--md > [class*=col]": {
                    "flex": "0 1 20%"
                },
                ".row.even-8--md > [class*=col]": {
                    "flex": "0 1 12.5%"
                },
                ".row.even-12--lg > [class*=col]": {
                    "flex": "0 1 8.3333333333%"
                },
                ".row.even-12--xl > [class*=col]": {
                    "flex": "0 1 8.3333333333%"
                }
            }
        }
    ],
